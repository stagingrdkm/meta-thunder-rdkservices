Subject: [PATCH] [ONEM-38627] Extending the error code enums

---
 interfaces/IDRM.h  | 4 ++++
 interfaces/IOCDM.h | 4 ++++
 2 files changed, 8 insertions(+)

diff --git a/interfaces/IDRM.h b/interfaces/IDRM.h
index f0114ff..2e81658 100644
--- a/interfaces/IDRM.h
+++ b/interfaces/IDRM.h
@@ -281,6 +281,7 @@ typedef enum {
     CDMi_MORE_DATA_AVAILABLE = 2,
     CDMi_INTERFACE_NOT_IMPLEMENTED = 3,
     CDMi_BUFFER_TOO_SMALL = 4,
+    CDMi_INIT_FAILED  = 5,
     CDMi_INVALID_ACCESSOR = 0x80000001,
     CDMi_KEYSYSTEM_NOT_SUPPORTED = 0x80000002,
     CDMi_INVALID_SESSION = 0x80000003,
@@ -289,9 +290,12 @@ typedef enum {
     CDMi_METHOD_NOT_IMPLEMENTED = 0x80000006,
     CDMi_FAIL = 0x80004005,
     CDMi_INVALID_ARG = 0x80070057,
+    CDMi_LICENSE_EXPIRED = 0x8004C009,
+    CDMi_LICENSE_NOT_FOUND = 0x8004C013,
     CDMi_SERVER_INTERNAL_ERROR = 0x8004C600,
     CDMi_SERVER_INVALID_MESSAGE = 0x8004C601,
     CDMi_SERVER_SERVICE_SPECIFIC = 0x8004C604,
+    CDMi_TEE_OUTPUT_PROTECTION_REQUIREMENTS_NOT_MET = 0x8004CD22,
     CDMi_BUSY_CANNOT_INITIALIZE = 0x8004DD00,
 } CDMi_RESULT;
 
diff --git a/interfaces/IOCDM.h b/interfaces/IOCDM.h
index 9dea324..c420caf 100644
--- a/interfaces/IOCDM.h
+++ b/interfaces/IOCDM.h
@@ -30,6 +30,7 @@ enum OCDM_RESULT : uint32_t {
     OCDM_MORE_DATA_AVAILABLE = 2,
     OCDM_INTERFACE_NOT_IMPLEMENTED = 3,
     OCDM_BUFFER_TOO_SMALL = 4,
+    OCDM_INIT_FAILED  = 5,
     OCDM_INVALID_ACCESSOR = 0x80000001,
     OCDM_KEYSYSTEM_NOT_SUPPORTED = 0x80000002,
     OCDM_INVALID_SESSION = 0x80000003,
@@ -38,9 +39,12 @@ enum OCDM_RESULT : uint32_t {
     OCDM_METHOD_NOT_IMPLEMENTED = 0x80000006,
     OCDM_FAIL = 0x80004005,
     OCDM_INVALID_ARG = 0x80070057,
+    OCDM_LICENSE_EXPIRED = 0x8004C009,
+    OCDM_LICENSE_NOT_FOUND = 0x8004C013,
     OCDM_SERVER_INTERNAL_ERROR = 0x8004C600,
     OCDM_SERVER_INVALID_MESSAGE = 0x8004C601,
     OCDM_SERVER_SERVICE_SPECIFIC = 0x8004C604,
+    OCDM_TEE_OUTPUT_PROTECTION_REQUIREMENTS_NOT_MET = 0x8004CD22,
     OCDM_BUSY_CANNOT_INITIALIZE = 0x8004DD00
 };
 
